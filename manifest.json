{
  "manifest_version": 2,
  "name": "__MSG_extensionName__",
  "description": "__MSG_extensionDescription__",
  "version": "1.2.0",
  "author": "Julia Kalder <mail@julia-kalder.de>",
  "homepage_url": "https://github.com/JuliaKalder/TemplateWing",
  "default_locale": "en",

  "browser_specific_settings": {
    "gecko": {
      "id": "templatewing@julia-kalder.de",
      "strict_min_version": "128.0"
    }
  },

  "icons": {
    "16": "images/icon-16.png",
    "32": "images/icon-32.png",
    "64": "images/icon-64.png",
    "128": "images/icon-128.png"
  },

  "background": {
    "page": "background.html"
  },

  "compose_action": {
    "default_popup": "popup/popup.html",
    "default_title": "__MSG_composeActionTitle__",
    "default_icon": {
      "16": "images/icon-16.png",
      "32": "images/icon-32.png"
    }
  },

  "options_ui": {
    "page": "options/options.html",
    "open_in_tab": true
  },

  "permissions": [
    "compose",
    "storage",
    "menus",
    "unlimitedStorage",
    "accountsRead"
  ],

  "commands": {
    "insert-template-1": {
      "suggested_key": { "default": "Ctrl+Shift+1" },
      "description": "Insert template 1"
    },
    "insert-template-2": {
      "suggested_key": { "default": "Ctrl+Shift+2" },
      "description": "Insert template 2"
    },
    "insert-template-3": {
      "suggested_key": { "default": "Ctrl+Shift+3" },
      "description": "Insert template 3"
    },
    "insert-template-4": {
      "suggested_key": { "default": "Ctrl+Shift+4" },
      "description": "Insert template 4"
    },
    "insert-template-5": {
      "suggested_key": { "default": "Ctrl+Shift+5" },
      "description": "Insert template 5"
    },
    "insert-template-6": {
      "suggested_key": { "default": "Ctrl+Shift+6" },
      "description": "Insert template 6"
    },
    "insert-template-7": {
      "suggested_key": { "default": "Ctrl+Shift+7" },
      "description": "Insert template 7"
    },
    "insert-template-8": {
      "suggested_key": { "default": "Ctrl+Shift+8" },
      "description": "Insert template 8"
    },
    "insert-template-9": {
      "suggested_key": { "default": "Ctrl+Shift+9" },
      "description": "Insert template 9"
    }
  }
}
